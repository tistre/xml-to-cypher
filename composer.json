{
  "name": "strehle-de/xml-to-cypher",
  "type": "library",
  "description": "Convert a custom XML format to Neo4j Cypher statements",
  "homepage": "https://github.com/tistre/xml-to-cypher",
  "license": "MIT",
  "authors": [
    {
      "name": "Tim Strehle",
      "homepage": "https://github.com/tistre",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=8.1 <8.3",
    "ext-dom": "*",
    "ext-json": "*",
    "ext-xmlreader": "*",
    "twig/twig": "^3.0"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "StrehleDe\\XmlToCypher\\": "src/"
    }
  }
}
